<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Read_and_learn.Page.MasterFlyoutPage"
             xmlns:pages="clr-namespace:Read_and_learn.Page">
  <FlyoutPage.Flyout>
    <pages:MasterFlyoutPageDetail x:Name="MasterPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:HomePage />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>